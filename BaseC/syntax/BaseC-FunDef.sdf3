module BaseC-FunDef

imports

  BaseC-Identifiers
  BaseC-Declarations
  BaseC-Stmt

context-free syntax

  Declaration.FunDeclaration = FunDef

  FunName.IdentifierName = Identifier
  FunName.ParenName      = <(<Identifier>)>

  FunDeclarator.FunDeclarator = <<Pointer?> <FunName> (<ParameterList?>)>

  FunDef.FunDef = <
    <DeclarationSpecifier*>
    <FunDeclarator>
    <Declaration*>
    <CompoundStatement>
  >
