module generate-cspm

strategies

  generate-cspm = fail

  can-generate-cspm = fail

  generate-cspm: t@(selected, position, ast, path, project-path) -> (file-name, dot)
    where
      file-name := <guarantee-extension(|"csp")> path
      ; dot := <
        collect-all(can-generate-cspm)
        ; map(generate-cspm)
        // for now, just create one graph
        ; Hd> ast
