module expressions

language MetaC

test desugar comma expression [[
  int32 a = (1, 2, 3);
]] run strip-annos to Program(_, [
    VarDeclaration(
      [],
      Type([],Int32),
      _,
      _,
      Some(Paren(
        Comma(
          Integer("1"),
          [Integer("2")],
          Integer("3")
        )
      ))
    )
  ])

test desugar cast types [[
  uint64 **a = [[(unsigned long * const *) 1]];
]] run strip-annos to Cast(
    Type([Const], Pointer(Pointer(UInt64()))),
    _
  )
